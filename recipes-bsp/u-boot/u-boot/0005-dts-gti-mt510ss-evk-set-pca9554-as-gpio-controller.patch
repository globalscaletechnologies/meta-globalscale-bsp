From e36e4c2d5dc4ebd10e6270ac400d4e9cdf422d35 Mon Sep 17 00:00:00 2001
From: Jason Hung <jhung@globalscaletechnologies.com>
Date: Fri, 13 Sep 2024 14:30:04 +0800
Subject: [PATCH 1/2] dts: gti-mt510ss-evk: set pca9554 as gpio controller

set pca9554 device as gpio controller
---
 arch/arm/dts/gti-mt510ss-evk.dts | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/arm/dts/gti-mt510ss-evk.dts b/arch/arm/dts/gti-mt510ss-evk.dts
index 5820e0866d..b5a78fe972 100644
--- a/arch/arm/dts/gti-mt510ss-evk.dts
+++ b/arch/arm/dts/gti-mt510ss-evk.dts
@@ -289,6 +289,8 @@
 
 	pca9554: gpio@3b {
 		compatible = "nxp,pca9554";
+		gpio-controller;
+		#gpio-cells = <2>;
 		reg = <0x3b>;
 		status = "okay";
 	};
