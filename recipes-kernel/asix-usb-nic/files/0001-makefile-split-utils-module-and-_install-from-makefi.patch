From: Jason Hung <jhung@globalscaletechnologies.com>
Date: Mon, 12 Aug 2024 09:28:16 +0800
Subject: [PATCH 1/2] makefile: split utils, module and _install from makefile

---
 Makefile | 11 +++++++++--
 1 file changed, 9 insertions(+), 2 deletions(-)

diff --git a/Makefile b/Makefile
index 3208f3a..54aa713 100755
--- a/Makefile
+++ b/Makefile
@@ -71,8 +71,15 @@ else
 MDIR	= kernel/drivers/net/usb
 endif
 
-all:
-	make -C $(KDIR) M=$(PWD) modules
+all: modules utils
+
+modules:
+	$(MAKE) -C $(KDIR) M=$(PWD) modules
+
+modules_install:
+	$(MAKE) -C $(KDIR) M=$(PWD) modules_install
+
+utils:
 	$(CC) $(TOOL_EXTRA_CFLAGS) ax88179_programmer.c -o ax88179_programmer
 	$(CC) $(TOOL_EXTRA_CFLAGS) ax88179a_programmer.c -o ax88179b_179a_772e_772d_programmer
 	$(CC) $(TOOL_EXTRA_CFLAGS) ax88279_programmer.c -o ax88279_programmer
